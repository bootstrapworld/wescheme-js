{
  "name": "wescheme-js",
  "version": "0.0.1",
  "description": "wescheme javascript compiler",
  "main": "lib/wescheme.js",
  "devDependencies": {
    "babel-cli": "^6.1.18",
    "babel-core": "^6.1.21",
    "babel-eslint": "^4.1.5",
    "babel-loader": "^6.1.0",
    "babel-preset-es2015": "^6.1.18",
    "codemirror": "^5.8.0",
    "css-loader": "^0.23.0",
    "eslint": "^1.9.0",
    "eslint-loader": "^1.1.1",
    "pretty-js": "^0.1.8",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.6",
    "webpack-dev-server": "^1.12.1"
  },
  "scripts": {
    "compile": "cp -r src/ lib && webpack --config webpack-package.config.js && ./node_modules/babel-cli/bin/babel.js --only src/wescheme.js -D -d lib/ src/",
    "prepublish": "npm run compile",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --content-base example/",
    "lint": "eslint . --ext .js || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pcardune/wescheme-js.git"
  },
  "author": "",
  "license": "LGPL",
  "bugs": {
    "url": "https://github.com/pcardune/wescheme-js/issues"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "only": [
      "bundle.js",
      "src/wescheme.js",
      "example/*"
    ]
  },
  "homepage": "https://github.com/pcardune/wescheme-js"
}
